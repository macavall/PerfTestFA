{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Trace",
      "Host.Results": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace",
      "Microsoft": "Trace",
      "Host.Aggregator": "Trace",
      "Function": "Trace",
      "Host.Executor": "Trace"
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request,Trace"
        }
      }
    }
  }
